        <errorConditionFormula>AND(OR(ISNEW(),NOT(ISPICKVAL(PRIORVALUE(MPOT_Anchor__c),&apos;&apos;))),((ISPICKVAL(MPOT_Anchor__c,&apos;&apos;)) &amp;&amp; RecordType.Name = &apos;MPOT Project&apos;))</errorConditionFormula>
